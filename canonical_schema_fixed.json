  "field_definitions": {
    "CORPORATE": {
      "revenue_gross": "Gross sales/revenue BEFORE any deductions (sales tax, excise, rebates, discounts)",
      "revenue_deductions": "Sales tax, excise duty, rebates, discounts, commissions deducted from gross revenue",
      "revenue_net": "Net sales/revenue AFTER all deductions. This is the primary revenue figure.",
      "revenue_local": "Revenue from local/domestic sales (geographic segment)",
      "revenue_export": "Revenue from export sales (geographic segment)",
      "cost_of_revenue": "Cost of goods sold / Cost of sales / Operating expenses (for oil & gas)",
      "royalties": "Government royalties, levies on production (common in oil & gas, mining)",
      "exploration_cost": "Exploration and prospecting costs (oil & gas, mining)",
      "other_cost_adjustments": "Any other costs between revenue and gross profit not captured by the above",
      "gross_profit": "Revenue - Cost of Revenue + Other Cost Adjustments",
      "operating_expenses": "Aggregate of selling, administrative, and other operating expenses (includes embedded depreciation)",
      "other_operating": "PLUG: operating_profit - gross_profit + operating_expenses (captures unlabeled items)",
      "operating_profit": "Gross Profit - Operating Expenses + Other Operating (= EBIT in most cases)",
      "other_income": "Non-operating income (dividend income, rental income, etc.)",
      "finance_cost": "Interest expense on borrowings",
      "other_non_operating": "PLUG: profit_before_tax - operating_profit - other_income + finance_cost",
      "profit_before_levy": "Profit before levy and taxation (if levy is shown separately). Maps to 'Profit before Taxation and Levy'.",
      "levy": "Workers' Welfare Fund, Workers' Profit Participation Fund, and similar levies (distinct from income tax)",
      "taxation_current": "Current period income tax expense (component of taxation)",
      "taxation_deferred": "Deferred tax expense/benefit (component of taxation)",
      "taxation_prior": "Prior period tax adjustment (component of taxation)",
      "taxation": "Total income tax expense. Map ONLY the total, not components (current/deferred/prior go to their own fields)",
      "net_profit_continuing": "Profit after tax from continuing operations (before discontinued operations)",
      "share_of_associates": "Share of profit/loss from associates accounted under equity method",
      "share_of_joint_ventures": "Share of profit/loss from joint ventures accounted under equity method",
      "net_profit_discontinued": "Profit/loss from discontinued operations (after tax)",
      "gain_on_disposal": "Gain on disposal of subsidiary, associate, or segment",
      "loss_on_disposal": "Loss on disposal of subsidiary, associate, or segment",
      "net_profit": "TOTAL profit after taxation (before attribution split). Use this for company-level metrics.",
      "net_profit_parent": "Profit attributable to equity holders of parent/holding company. Use this for EPS calculations.",
      "nci_income": "Profit attributable to non-controlling interests (minority shareholders)",
      "depreciation_amortization": "Total D&A from Cash Flow Statement (authoritative source)",
      "net_profit_cf_start": "Starting point for cash flow (should equal net_profit from P&L)",
      "gain_on_sale_fixed_assets": "Gain/loss on disposal of fixed assets (non-cash adjustment in CFO)",
      "unrealized_gain_on_investments": "Unrealized gain/loss on investments marked to market (non-cash)",
      "fair_value_gains_losses": "Fair value adjustments on financial instruments",
      "share_of_profit_associates": "Share of profit/loss from associates and joint ventures",
      "provisions_cf": "Provisions and reversals (non-cash adjustments in CFO)",
      "other_operating_adjustments": "Individual non-cash adjustments in operating section (provisions, gains/losses, etc.) - NOT the section total",
      "interest_received": "Interest/markup received (cash inflow)",
      "finance_cost_paid": "Interest/finance cost actually paid (cash basis)",
      "taxes_paid": "Income taxes actually paid (cash basis)",
      "cfo": "SECTION TOTAL ONLY: 'Net cash from operating activities' or 'Cash generated from operations'. Map ONLY the final operating section total, NOT intermediate subtotals or individual line items.",
      "capex": "Capital expenditures - purchases of property, plant, equipment, and intangibles",
      "acquisitions": "Cash paid for business acquisitions and subsidiary purchases",
      "asset_sales": "Proceeds from sale of fixed assets and subsidiaries",
      "investment_purchases": "Purchases of investments, securities, term deposits",
      "investment_sales": "Proceeds from sale of investments, securities, term deposits",
      "dividend_received": "Dividends received from investments",
      "gain_on_securities": "Realized gain/loss on sale of securities",
      "other_investing": "Other investing activities not covered by capex/acquisitions/sales/investments",
      "cfi": "SECTION TOTAL ONLY: 'Net cash from investing activities'. Map ONLY the final investing section total, NOT individual line items.",
      "borrowings_raised": "Proceeds from new borrowings, loans, debt issuance",
      "borrowings_repaid": "Repayment of borrowings, loans, debt principal",
      "borrowings_raised_repaid": "Net borrowings when raised and repaid are combined in one line",
      "change_in_borrowings": "Net change in borrowings from financial institutions",
      "dividends_paid": "Dividends paid to shareholders",
      "interest_expensed_lease": "Interest/finance cost on lease liabilities",
      "lease_principal_paid": "Principal payments on lease liabilities",
      "gain_on_lease_termination": "Gain/loss on lease termination or modification",
      "other_financing": "Other financing activities (lease payments, transaction costs, etc.)",
      "cff": "SECTION TOTAL ONLY: 'Net cash from financing activities'. Map ONLY the final financing section total, NOT individual line items.",
      "fx_adjustment": "Effects of exchange rate changes on cash and cash equivalents (foreign currency translation)",
      "cash_start": "Cash and cash equivalents at beginning of period",
      "net_cash_change": "SECTION TOTAL: Net increase/decrease in cash and cash equivalents for the period",
      "cash_end": "Cash and cash equivalents at end of period"
    },
    "BANK": {
      "bank_interest_income": "Mark-up/return/interest earned (gross)",
      "bank_interest_expense": "Mark-up/return/interest expensed",
      "net_interest_income": "Bank Interest Income - Bank Interest Expense",
      "total_income": "Net Interest Income + Non-Interest Income",
      "provisions": "Credit loss provisions and write-offs (net)",
      "other_bank_adjustments": "PLUG: profit_before_tax - total_income + operating_expenses + provisions + other_charges",
      "lending_to_fis": "Lendings to financial institutions",
      "bank_balances": "Balances with other banks",
      "interest_expensed_subdebt": "Interest on subordinated debt",
      "gain_loss_derecognition": "Gain/loss on derecognition of financial assets",
      "exchange_gain_goodwill": "Exchange gain/loss on goodwill",
      "exchange_gain_repatriation": "Exchange gain on branch closure/repatriation",
      "defined_benefits_paid": "Contributions to defined benefit plans"
    },
    "INSURANCE": {
      "gross_premium": "Total premiums written before reinsurance",
      "net_premium": "Gross premium - reinsurance expense",
      "claims_expense": "Insurance claims paid/accrued",
      "other_underwriting": "PLUG: underwriting_profit - net_premium + claims_expense + commission_expense",
      "underwriting_profit": "Net Premium - Claims - Commission + Other Underwriting",
      "other_insurance_adjustments": "PLUG: profit_before_tax - underwriting_profit - investment_income - other_income + operating_expenses"
    }
  },
  "extraction_notes": {
    "depreciation_source": "Always extract from Cash Flow Statement, not P&L. P&L depreciation is embedded in operating expenses and unreliable.",
    "revenue_mapping": "Map gross revenue to revenue_gross, deductions to revenue_deductions, net revenue to revenue_net. Do NOT map multiple overlapping revenue figures to the same field.",
    "revenue_segments": "Local/Export breakdowns go to revenue_local/revenue_export, not revenue_net",
    "subtotals": "Do NOT map generic 'Total' or 'Sub-total' rows - these are intermediate calculations",
    "ppe_sub_components": "Property, plant and equipment (PPE) is often presented with sub-components. Use distinct canonicals: operating_fixed_assets (main PPE), capital_work_in_progress (CWIP), major_spare_parts (spare parts inventory), development_production_assets (oil & gas), development_decommissioning_costs (oil & gas), furniture_equipment (service companies), right_of_use_assets (leases). The subtotal line should use property_equipment with a formula. Do NOT map all sub-components to property_equipment.",
    "operating_cash_flow_definition": "Use NET operating cash flow (after interest and taxes paid), not 'cash generated from operations'",
    "cost_terminology": "Use 'cost_of_revenue' to match Step7 implementation",
    "taxation_mapping": "Map total taxation to 'taxation', current component to 'taxation_current', deferred to 'taxation_deferred'. Do NOT map both total and components to 'taxation'.",
    "levy_vs_tax": "Levy (WWF, WPPF) is separate from income tax. Map to 'levy', not 'taxation'.",
    "operating_expenses_rule": "Map individual expense lines (admin, selling, distribution) to operating_expenses. Do NOT also map 'Total Expenses' or 'Total Operating Expenses' - choose components OR total, not both.",
    "net_profit_attribution": "Map TOTAL 'Profit after taxation' to net_profit. Map 'Attributable to owners/parent' to net_profit_parent. Map 'Attributable to NCI' to nci_income. Do NOT map attribution lines to net_profit.",
    "discontinued_operations": "When P&L shows continuing/discontinued split: Map 'Profit from continuing operations' to net_profit_continuing, 'Profit from discontinued operations' to net_profit_discontinued, and TOTAL to net_profit. The equation should be: net_profit = net_profit_continuing + net_profit_discontinued (+ share_of_associates if shown after continuing).",
    "share_of_associates_position": "Share of profit from associates can appear either BEFORE profit_before_tax (more common) or AFTER net_profit_continuing (for companies with discontinued ops). Map to share_of_associates in either case.",
    "cash_flow_section_totals": "CRITICAL: cfo, cfi, cff are SECTION TOTALS only. Map ONLY lines that say 'Net cash from operating/investing/financing activities' or similar. Individual items within each section (premiums received, claims paid, capex, loan proceeds, etc.) should be mapped to their specific canonicals (other_operating_adjustments, capex, borrowings_raised, etc.), NOT to the section total. There should be exactly ONE cfo, ONE cfi, and ONE cff per cash flow statement.",
    "borrowings_combined": "When borrowings raised and repaid are shown as a single net line, use borrowings_raised_repaid instead of borrowings_raised or borrowings_repaid",
    "cash_positions": "Extract cash_start (beginning balance), net_cash_change (movement), and cash_end (ending balance) when available"
  }
